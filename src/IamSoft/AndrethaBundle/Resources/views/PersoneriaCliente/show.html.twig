{% extends 'IamSoftAndrethaBundle:Default:base.html.twig' %}

{%block content%}

<h1>PersoneriaCliente</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Personeria</th>
            <td>{{ entity.personeria }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('personeriacliente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('personeriacliente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('personeriacliente_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

{%endblock%}

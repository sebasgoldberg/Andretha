{% extends 'IamSoftAndrethaBundle:Default:base.html.twig' %}

{%block content%}
<h1>Libro list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Numero</th>
            <th>Registro escribano</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('libro_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.numero }}</td>
            <td>{{ entity.registroEscribano }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('libro_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('libro_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('libro_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{%endblock%}
